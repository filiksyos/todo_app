struct account_dto {
    id: byte_array;
    type: text;
}

struct asset_dto {
    id: byte_array;
    name: text;
    symbol: text;
    decimals: integer;
    blockchain_rid: byte_array;
    icon_url: text;
    type: text;
    supply: big_integer;
}

function account_from_gtv(paged_result) = account_dto.from_gtv_pretty(paged_result.data[0]);

function asset_from_gtv(paged_result) = asset_dto.from_gtv_pretty(paged_result.data[0]);
